<h1>Users</h1>

<table>
<thead>
	<tr>
		<th>image</th>
		<th>name</th>
		<th><th>
	</tr>
	</thead>
	<tr>
		<% @users.each do |user| %>
	<tbody>
		<td><%= attachment_image_tag user, :profile_image, size: "50x50", fallback: "no_image.jpg" %>
		<td><%= user.name %></td>
		<td><%= link_to "show", user_path(user.id) %></td>
	</tbody>
</tr>
<% end %>
</table>

<%= render "side", user: @user, book: @newbook %>